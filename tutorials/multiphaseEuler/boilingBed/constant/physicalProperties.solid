FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.solid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        molWeight   55.85;
    }
    equationOfState
    {
        rho         8072;
    }
    thermodynamics
    {
        Hf          0;
        Cv          520;
    }
    transport
    {
        mu          1;
        kappa       16.7;
    }
}

