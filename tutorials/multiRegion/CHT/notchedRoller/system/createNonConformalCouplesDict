FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      createNonConformalCouplesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fields          no;

nonConformalCouples
{
    fluidFluid
    {
        region          fluid;
        patches         (nonCoupleRotating nonCoupleStationary);
    }

    solidFluid
    {
        regions         (solid fluid);
        patches         (nonCoupleStationary nonCoupleRotating);
    }

    rollerFluid
    {
        regions         (roller fluid);
        patches         (nonCoupleRotating nonCoupleStationary);
    }

    solidRoller
    {
        regions         (solid roller);
        patches         (nonCoupleStationary nonCoupleRotating);
    }
}

