FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    fluid
    {
        type            externalTemperature;
        Ta              constant 300.0;
        h               uniform 100;
        qr              qr;
        value           $internalField;
    }

    wall
    {
        type            zeroGradient;
    }

    mapped
    {
        type            coupledTemperature;
        qr              qr;
        value           $internalField;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


// ************************************************************************* //
