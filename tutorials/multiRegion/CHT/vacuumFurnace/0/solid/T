FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    wall
    {
        type            zeroGradient;
    }

    mapped
    {
        type            coupledTemperature;
        value           $internalField;
        qrNbr           qr;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


