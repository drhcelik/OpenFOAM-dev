FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regionSolvers
{
    solid       solid;
    heater      solid;
    shield      solid;
    fluid       fluid;
};

startTime       latestTime;

stopAt          endTime;

endTime         10000;

deltaT          200;

writeControl    runTime;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable true;

// ************************************************************************* //
