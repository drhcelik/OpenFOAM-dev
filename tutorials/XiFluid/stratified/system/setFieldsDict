FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    fu.u  0;
    egr.u 0;
}

zones
{
    fuelAir
    {
        type        box;

        box         (0 0 -1) (1 0.01 1);

        values
        {
            fu.u  0.07;
            egr.u 0.01;
        }
    }
}

