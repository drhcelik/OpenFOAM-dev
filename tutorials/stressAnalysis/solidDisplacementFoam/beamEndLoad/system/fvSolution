FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    D
    {
        solver          GAMG;
        smoother        GaussSeidel;

        tolerance       1e-10;
        relTol          0.99;

        nCellsInCoarsestLevel 6;
    }
}

stressAnalysis
{
    nCorrectors     1;
    D               1e-10;
    // accelerationFactor 2;   // lower for other Poisson ratio (e.g. 0.1 for 0.3)
    compactNormalStress no;
}


