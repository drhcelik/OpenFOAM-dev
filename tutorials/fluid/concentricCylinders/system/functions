#includeFunc patchIntegrate(name=qrOuter, patch=outer, qr)
#includeFunc patchIntegrate(name=qInner, patch=inner, wallHeatFlux)
#includeFunc patchIntegrate(name=qOuter, patch=outer, wallHeatFlux)

