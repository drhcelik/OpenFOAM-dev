FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      G;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wall
    {
        type            MarshakRadiation;
        emissivityMode  lookup;
        emissivity      uniform 0.5;
        value           uniform 0;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

