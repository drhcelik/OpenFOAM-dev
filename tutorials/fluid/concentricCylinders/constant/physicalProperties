thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   40;
    }
    thermodynamics
    {
        Cp          520;
        hf          0;
    }
    transport
    {
        mu          3.9e-5;
        kappa       0.031;
    }
}

