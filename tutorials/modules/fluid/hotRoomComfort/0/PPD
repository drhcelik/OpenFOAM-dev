FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      PPD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 46.0115;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 46.0115;
    }
    inlet
    {
        type            calculated;
        value           uniform 34.4876;
    }
    outlet
    {
        type            calculated;
        value           uniform 46.0115;
    }
}


